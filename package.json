{
  "name": "pm2-logrotate-s3-upload",
  "version": "1.0.0",
  "description": "Module to rotate logs of every pm2 application",
  "main": "app.js",
  "dependencies": {
    "moment-timezone": "0.5.5",
    "node-schedule": "1.1.1",
    "pm2": "latest",
    "pmx": "latest"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Himanshu Pandey",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himanshu0511/pm2-logrotate-s3-upload.git"
  },
  "bugs": {
    "url": "https://github.com/himanshu0511/pm2-logrotate-s3-upload/issues"
  },
  "homepage": "https://github.com/himanshu0511/pm2-logrotate-s3-upload",
  "license": "MIT",
  "apps": [
    {
      "name": "pm2-logrotate",
      "script": "app.js"
    }
  ],
  "config": {
    "max_size": "10M",
    "retain": "all",
    "compress": false,
    "dateFormat": "YYYY-MM-DD_HH-mm-ss",
    "workerInterval": "30",
    "rotateInterval": "0 0 * * *",
    "rotateModule": true,
    "aws": {
      "credentials": {
        "accessKeyId": null,
        "secretAccessKey": null,
        "region": null
      }
    },
    "logBucketSetting": {
      "bucket": null,
      "s3Path": null,
      "acl": null,
      "expiry": null
    }
  }
}
